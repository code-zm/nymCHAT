# Unique identifier for this discovery node
NYM_CLIENT_ID=discovery_node

# Path to the Nym SDK storage directory (defaults to storage/${NYM_CLIENT_ID} if unset)
#NYM_SDK_STORAGE=storage/${NYM_CLIENT_ID}

# Path to the SQLite database file
DATABASE_PATH=db/discovery_node.db

# Directory for user encryption keys
KEYS_DIR=keys

# Path to the file containing your encryption password
SECRET_PATH=secrets/encryption_password

# File path for log output
LOG_FILE_PATH=logs/discovery_node.log